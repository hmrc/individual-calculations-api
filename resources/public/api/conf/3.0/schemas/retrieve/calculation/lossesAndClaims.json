{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Object containing information about losses and claims in the retrieved tax calculation.",
  "type": "object",
  "properties": {
    "resultOfClaimsApplied": {
      "description": "Array detailing the result of claims applied.",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "claimId": {
            "description": "The unique identifier of the claim.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "1234568790ABCDE"
          },
          "originatingClaimId": {
            "description": "The unique identifier of the original carry-sideways-fhl/carry-sideways claim where not all of the loss in the calculation year could be carried sideways and the resulting loss balance was carried forwards. Where a resulting carry sideways loss balance is carried forwards there will not be a new claim ID for the carried forward portion of the loss.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "1234568790ABCDE"
          },
          "incomeSourceId": {
            "description": "The unique identifier of the income source.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "XAIP12345678910"
          },
          "incomeSourceType": {
            "description": "The income source type for the claim.",
            "enum": [
              "self-employment",
              "uk-property-non-fhl",
              "foreign-property-fhl-eea",
              "uk-property-fhl",
              "foreign-property"
            ],
            "type": "string"
          },
          "taxYearClaimMade": {
            "description": "The tax year in which the claim was made. Must conform to the format: YYYY-YY",
            "type": "string",
            "example": "2020-21"
          },
          "claimType": {
            "description": "Type of claim.",
            "enum": [
              "carry-forward",
              "carry-sideways",
              "carry-forward-to-carry-sideways",
              "carry-sideways-fhl",
              "carry-backwards",
              "carry-backwards-general-income"
            ],
            "type": "string"
          },
          "mtdLoss": {
            "description": "Indicates whether the loss was registered by the taxpayer on joining MTD. The value must be false.",
            "type": "boolean",
            "example": "false"
          },
          "taxYearLossIncurred": {
            "description": "The tax year in which the loss was incurred. Must conform to the format: YYYY-YY",
            "type": "string",
            "example": "2020-21"
          },
          "lossAmountUsed": {
            "description": "Loss amount used. The value must be between 0 and 99999999999. No decimals.",
            "type": "integer",
            "multipleOf": 1,
            "minimum": 0,
            "maximum": 99999999999,
            "example": "160"
          },
          "remainingLossValue": {
            "description": "Remaining loss value. The value must be between 0 and 99999999999. No decimals.",
            "type": "integer",
            "multipleOf": 1,
            "minimum": 0,
            "maximum": 99999999999,
            "example": "160"
          },
          "lossType": {
            "description": "Type of loss.",
            "enum": [
              "income",
              "class4nics"
            ],
            "type": "string"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "incomeSourceId",
        "incomeSourceType",
        "taxYearClaimMade",
        "claimType",
        "taxYearLossIncurred",
        "lossAmountUsed",
        "remainingLossValue"
      ]
    },
    "unclaimedLosses": {
      "description": "Array detailing unclaimed losses.",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "incomeSourceId": {
            "description": "The unique identifier of the income source.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "XAIP12345678910"
          },
          "incomeSourceType": {
            "description": "The income source type for the unclaimed loss.",
            "enum": [
              "self-employment",
              "uk-property-non-fhl",
              "foreign-property-fhl-eea",
              "uk-property-fhl",
              "foreign-property"
            ],
            "type": "string"
          },
          "taxYearLossIncurred": {
            "description": "The tax year in which the loss was incurred. Must conform to the format: YYYY-YY",
            "type": "string",
            "example": "2020-21"
          },
          "currentLossValue": {
            "description": "Current loss value. The value must be between 0 and 99999999999. No decimals.",
            "type": "integer",
            "multipleOf": 1,
            "minimum": 0,
            "maximum": 99999999999,
            "example": "160"
          },
          "lossType": {
            "description": "Type of loss.",
            "enum": [
              "income",
              "class4nics"
            ],
            "type": "string"
          }
        },
        "additionalProperties": false,
        "required": [
          "incomeSourceType",
          "taxYearLossIncurred",
          "currentLossValue"
        ]
      }
    },
    "carriedForwardLosses": {
      "description": "Array detailing carried forward losses.",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "claimId": {
            "description": "The unique identifier of the claim.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "1234568790ABCDE"
          },
          "originatingClaimId": {
            "description": "The unique identifier of the originating claim.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "1234568790ABCDE"
          },
          "incomeSourceId": {
            "description": "The unique identifier of the income source.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "XAIS12345678910"
          },
          "incomeSourceType": {
            "description": "The income source type for the carried forward loss.",
            "enum": [
              "self-employment",
              "uk-property-non-fhl",
              "foreign-property-fhl-eea",
              "uk-property-fhl",
              "foreign-property"
            ],
            "type": "string"
          },
          "claimType": {
            "description": "Type of claim.",
            "enum": [
              "carry-forward",
              "carry-sideways",
              "carry-forward-to-carry-sideways",
              "carry-sideways-fhl",
              "carry-backwards",
              "carry-backwards-general-income"
            ],
            "type": "string"
          },
          "taxYearClaimMade": {
            "description": "The tax year in which the claim was made. Must conform to the format: YYYY-YY",
            "type": "string",
            "example": "2020-21"
          },
          "taxYearLossIncurred": {
            "description": "The tax year in which the loss was incurred. Must conform to the format: YYYY-YY",
            "type": "string",
            "example": "2020-21"
          },
          "currentLossValue": {
            "description": "Current loss value. The value must be between 0 and 99999999999. No decimals.",
            "type": "integer",
            "multipleOf": 1,
            "minimum": 0,
            "maximum": 99999999999,
            "example": "160"
          },
          "lossType": {
            "description": "Type of loss.",
            "enum": [
              "income",
              "class4nics"
            ],
            "type": "string"
          }
        },
        "additionalProperties": false,
        "required": [
          "incomeSourceId",
          "incomeSourceType",
          "claimType",
          "taxYearLossIncurred",
          "currentLossValue"
        ]
      }
    },
    "defaultCarriedForwardLosses": {
      "description": "Losses arising in this calculation that have been carried forward to the next calculation by default. This applies to specific income sources e.g Furnished Holiday Lettings where losses can only be carried forward.",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "incomeSourceId": {
            "description": "The unique identifier of the income source.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "XAIS12345678910"
          },
          "incomeSourceType": {
            "description": "The income source type for the default carried forward loss.",
            "enum": [
              "self-employment",
              "uk-property-non-fhl",
              "foreign-property-fhl-eea",
              "uk-property-fhl",
              "foreign-property"
            ],
            "type": "string"
          },
          "taxYearLossIncurred": {
            "description": "The tax year in which the loss was incurred. Must conform to the format: YYYY-YY.",
            "type": "string",
            "example": "2020-21"
          },
          "currentLossValue": {
            "description": "The tax year in which the loss was incurred.",
            "type": "integer",
            "multipleOf": 1,
            "minimum": 0,
            "maximum": 99999999999,
            "example": "160"
          }
        },
        "additionalProperties": false,
        "required": [
          "incomeSourceId",
          "incomeSourceType",
          "taxYearLossIncurred",
          "currentLossValue"
        ]
      }
    },
    "claimsNotApplied": {
      "description": "Array detailing claims that could not be applied in this calculation.",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "claimId": {
            "description": "The unique identifier of the claim.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "1234568790ABCDE"
          },
          "incomeSourceId": {
            "description": "The unique identifier of the claim.",
            "type": "string",
            "pattern": "^[A-Za-z0-9]{15}$",
            "example": "XAIS12345678910"
          },
          "incomeSourceType": {
            "description": "The income source type for the claim not applied.",
            "enum": [
              "self-employment",
              "uk-property-non-fhl",
              "foreign-property-fhl-eea",
              "uk-property-fhl",
              "foreign-property"
            ],
            "type": "string"
          },
          "taxYearClaimMade": {
            "description": "The tax year in which the claim was made. Must conform to the format: YYYY-YY.",
            "type": "string",
            "example": "2020-21"
          },
          "claimType": {
            "description": "Type of claim.",
            "enum": [
              "carry-forward",
              "carry-sideways",
              "carry-forward-to-carry-sideways",
              "carry-sideways-fhl",
              "carry-backwards",
              "carry-backwards-general-income"
            ],
            "type": "string"
          }
        },
        "additionalProperties": false,
        "required": [
          "claimId",
          "incomeSourceId",
          "incomeSourceType",
          "taxYearClaimMade",
          "claimType"
        ]
      }
    }
  },
  "additionalProperties": false
}